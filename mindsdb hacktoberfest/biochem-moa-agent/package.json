{
  "name": "dietmoa-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for Dietary Mechanism of Action Prediction",
  "main": "mcp_server/server.js",
  "scripts": {
    "start": "node mcp_server/server.js",
    "dev": "nodemon mcp_server/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint mcp_server/**/*.js",
    "format": "prettier --write mcp_server/**/*.js",
    "build": "webpack --mode production",
    "start:mcp": "node dist/mcp-server.js",
    "dev:mcp": "nodemon --watch mcp_server --exec node mcp_server/server.js"
  },
  "keywords": [
    "mcp",
    "biochemistry",
    "drug-discovery",
    "pathway-analysis",
    "ai-agents"
  ],
  "author": "DietMOA Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "axios": "^1.6.2",
    "neo4j-driver": "^5.14.0",
    "ioredis": "^5.3.2",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/dietmoa-agent.git"
  },
  "bugs": {
